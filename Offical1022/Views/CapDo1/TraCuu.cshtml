                        nguoigoi_email: MyApp.nguoigoi_email,
                        nguoigoi_nickname: MyApp.nguoigoi_nickname,
                        mangxh_id: MyApp.mangxh_id,
                        loaiyc_id: MyApp.loaiyc_id,
