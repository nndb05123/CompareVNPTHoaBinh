                        return;
                    }
                    MyApp.nguoigoi_phuong_id = $('#cboPhuong').val();
                    var postData = {
                        yeucau_id: MyApp.yeucau_id,
                        yeucau_noidung: MyApp.yeucau_noidung,
                        nguoigoi_hoten: MyApp.nguoigoi_hoten,
